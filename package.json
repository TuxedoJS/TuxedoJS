{
  "name": "tux",
  "version": "0.0.0",
  "description": "Framework built on React and Flux",
  "main": "Tuxedo.js",
  "scripts": {
    "test": "jest",
    "start": "watchify --extension=.jsx -d Tuxedo.js -o bundle.js -v"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TuxedoJS/TuxedoJS"
  },
  "keywords": [
    "TuxedoJs",
    "Tux",
    "tux",
    "tuxedoJS",
    "tuxedo",
    "Tuxedo"
  ],
  "author": "drabinowitz, cheerazar, plauer, sjstebbins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TuxedoJS/TuxedoJS/issues"
  },
  "homepage": "https://github.com/TuxedoJS/TuxedoJS",
  "dependencies": {
    "browserify": "^8.0.2",
    "envify": "^3.2.0",
    "events": "^1.0.2",
    "flux": "^2.0.1",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-router": "^0.11.6",
    "react-validating-form": "^0.1.1",
    "reactify": "^0.17.1",
    "watchify": "^2.2.1"
  },
  "devDependencies": {
    "jest": "^0.1.37",
    "jest-cli": "^0.2.1",
    "react-tools": "^0.12.2"
  },
  "jest": {
    "scriptPreprocessor": "jest-preprocessor.js",
    "unmockedModulePathPatterns": [
      "src/TuxInvariant",
      "node_modules/react/",
      "node_modules/object-assign/"
    ]
  }
}
